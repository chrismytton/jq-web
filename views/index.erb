<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h1>jq(1) for the web</h1>

    <form method="get" action="<%= url('/jq') %>">
      <div class="form-group">
        <label for="url">URL to JSON file</label>
        <input type="url" name="json" id="url" class="form-control">
      </div>
      <div class="form-group">
        <label for="filter">jq filter</label>
        <input type="text" name="filter" id="filter" class="form-control">
      </div>
      <input type="submit" value="Query" class="btn btn-default">
    </form>
  </div>
</div>
